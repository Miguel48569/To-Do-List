{
  "name": "To Do List Anime",
  "theme_color": "#000000",
  "background_color": "#000000",
  "short_name": "ToDo Anime",
  "start_url": "/",
  "display": "standalone",
  "launch_handler": {
    "client_mode": "auto"
  },
  "orientation": "any",
  "description": "O Anime To-Do List é um aplicativo web projetado para ajudar fãs de anime a organizarem suas tarefas de forma divertida e produtiva.",
  "id": "ToDoListAnime",

  "serviceworker": {
    "src": "/sw.js",
    "scope": "/",
    "type": "module"
  },

  "icons": [
    {
      "src": "/img/pwa_icon-192-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },

    {
      "src": "/img/pwa_icon-512-512.png",
      "sizes": "512x512",
      "type": "image/png"
    },

    {
      "src": "/img/pwa_icon-72-72.png",
      "sizes": "72x72",
      "type": "image/png"
    },

    {
      "src": "/img/pwa_icon.png",
      "sizes": "48x48",
      "type": "image/png"
    },

    {
      "src": "/img/pwa_87.png",
      "sizes": "87x87",
      "type": "image/png"
    }
  ]
}
